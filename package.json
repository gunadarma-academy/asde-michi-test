{
  "name": "arlin-test",
  "version": "0.1.0",
  "description": "Arlin various tests",
  "main": "index.js",
  "scripts": {
    "start": "echo 'Use npm test, install:all, start:all, and test:all'",
    "cucumber": "cucumber.js features -r features/step_definitions",
    "test": "npm run cucumber -s",
    "install:backend": "cd backend && npm install",
    "install:frontend": "cd frontend && npm install",
    "install:all": "npm run install:backend && npm run install:frontend",
    "start:backend": "cd backend && npm start",
    "start:frontend": "cd frontend && npm start",
    "start:all": "npm run start:backend && npm run start:frontend",
    "test:backend": "cd frontend && npm test",
    "test:frontend": "cd backend && npm test",
    "test:all": "npm run test:backend && npm run test:frontend"
  },
  "homepage": "http://staging.arlin.link",
  "repository": {
    "type": "git",
    "url": "git+ssh://github.com/gunadarma-academy/asde-michi-test.git"
  },
  "bugs": {
